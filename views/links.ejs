<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="shortcut icon" href="images\undraw_share_link_qtxe.svg" type="image/x-icon">

    <!-- CSS only -->
    <link rel="stylesheet" href="css/bootstrap.min.css">

    <style>
        .form {
            margin-top: 120px;
        }

        body {
            margin: 0;
        }

        .userimage img {
            margin-bottom: 20px;
            margin-left: auto;
            margin-right: auto;
            display: block;
        }
    </style>


</head>

<body>

    <div class="container" style="margin-top: 100px;">
        <div class="row">
            <div class="col-lg-12 userimage">
                <img style="border-radius: 100%;" src="<%=ownerimage%>" alt="">
                <p style="text-align: center; margin-bottom: 20px;">@<%=owneronelink%></p>
            </div>
            <div class="col-lg-12 " style="max-width: 500px; margin: auto;">

                <%links.forEach(function(link){%>

                <a target="_blank" href="<%=link.link%>">
                    <button type="submit" data-toggle="modal" data-target="addlink"
                        style="height: 50px; width:100%;background-color: #8e87e4; margin-bottom: 20px;"
                        class="btn btn-primary" type="button"><%=link.title%></button>
                </a>

                <% }) %>
            </div>
        </div>
    </div>





    <!-- JavaScript Bundle with Popper -->
    <!-- jQuery -->
    <script src="js/jquery-3.5.1.min.js"></script>
    <!-- Bootstrap 4.5 JS -->
    <script src="js/bootstrap.min.js"></script>
</body>

</html>